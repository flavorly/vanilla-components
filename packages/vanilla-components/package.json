{
  "name": "@indigit/vanilla-components",
  "version": "0.7.2",
  "type": "module",
  "sideEffects": false,
  "private": false,
  "author": "Jon <support@indigit.pt>",
  "license": "ISC",
  "description": "ðŸ¤Œ A lightweight Vue 3 Component Library based on VariantJS & Headless UI",
  "keywords": [
    "component library",
    "ui components",
    "vuejs",
    "vue",
    "tailwind",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikuscs/vanilla-components"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/vanilla-components.umd.js",
  "module": "./dist/vanilla-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/vanilla-components.es.js",
      "require": "./dist/vanilla-components.umd.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:watch-mon": "nodemon --exec 'pnpm lib:build'",
    "lib:publish": "pnpm publish",
    "watch:both": "concurrently \"yarn && yarn watch:lib\" \"yarn && yarn watch:docs\"",
    "watch:docs": "pnpm --cwd \"docs\" dev",
    "lint": "eslint --ext .js,.vue --ignore-path ../../.gitignore --fix packages/src",
    "format": "prettier ../..  --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.3",
    "@popperjs/core": "^2.11.6",
    "@vueuse/core": "^9.3.1",
    "axios": "^1.1.3",
    "fuse.js": "^6.6.2",
    "libphonenumber-js": "^1.10.14",
    "lodash": "^4.17.21",
    "@indigit/v-calendar": "3.0.0-alpha.10",
    "vue": "^3.2.41"
  },
  "peerDependencies": {
    "@headlessui/vue": "^1.7.0",
    "@popperjs/core": "^2.11.6",
    "axios": "^1.1.2",
    "@indigit/v-calendar": "3.0.0-alpha.10",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.2",
    "@antfu/ni": "^0.18.2",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/lodash": "^4.14.186",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.41",
    "bumpp": "^8.2.1",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "eslint-plugin-vue": "^9.6.0",
    "pnpm": "^7.13.6",
    "prettier": "^2.7.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^1.6.6"
  }
}
