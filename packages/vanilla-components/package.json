{
  "name": "@flavorly/vanilla-components",
  "version": "0.7.26",
  "type": "module",
  "sideEffects": false,
  "private": false,
  "author": "Jon <support@flavorly.pt>",
  "license": "ISC",
  "description": "ðŸ¤Œ A lightweight Vue 3 Component Library based on VariantJS & Headless UI",
  "keywords": [
    "component library",
    "ui components",
    "vuejs",
    "vue",
    "tailwind",
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flavorly/vanilla-components"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/vanilla-components.umd.js",
  "module": "./dist/vanilla-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/vanilla-components.es.js",
      "require": "./dist/vanilla-components.umd.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:watch-mon": "nodemon --exec 'pnpm lib:build'",
    "lib:publish": "pnpm publish",
    "watch:both": "concurrently \"yarn && yarn watch:lib\" \"yarn && yarn watch:docs\"",
    "watch:docs": "pnpm --cwd \"docs\" dev",
    "lint": "eslint --ext .js,.vue --ignore-path ../../.gitignore --fix packages/src",
    "format": "prettier ../..  --write"
  },
  "dependencies": {
    "@flavorly/v-calendar": "3.0.0-alpha.11",
    "@headlessui/vue": "^1.7.4",
    "@popperjs/core": "^2.11.6",
    "@vueuse/core": "^9.5.0",
    "@vueuse/integrations": "^9.6.0",
    "axios": "^1.1.3",
    "focus-trap": "^7.1.0",
    "fuse.js": "^6.6.2",
    "libphonenumber-js": "^1.10.14",
    "lodash": "^4.17.21",
    "unplugin-vue-define-options": "^1.0.0",
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "@flavorly/v-calendar": "3.0.0-alpha.11",
    "@headlessui/vue": "^1.7.0",
    "@popperjs/core": "^2.11.6",
    "axios": "^1.1.2",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@antfu/ni": "^0.20.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/lodash": "^4.14.189",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "bumpp": "^8.2.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "eslint-plugin-unicorn": "^45.0.0",
    "eslint-plugin-vue": "^9.7.0",
    "pnpm": "^7.16.0",
    "prettier": "^2.7.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "typescript": "^4.8.4",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.1"
  }
}
